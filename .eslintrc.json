{
  "extends": ["eslint:recommended", "plugin:react/recommended", "prettier"],
  "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module",
      "ecmaFeatures": {
          "jsx": true
      }
  },
  "env": {
      "browser": true,
      "node": true,
      "es6": true
  },
  "settings": {
      "react": {
          "version": "detect"
      }
  },
  "plugins": ["react"]
}
